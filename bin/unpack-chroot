"""Unpack a chroot tarball."""

from __future__ import print_function

__metaclass__ = type

import sys

from lpbuildd.target.create import Create
from lpbuildd.target.operation import configure_logging


def main():
    configure_logging()
    return Create().run()


if __name__ == "__main__":
    sys.exit(main())
