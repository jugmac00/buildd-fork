"""Mount a chroot."""

from __future__ import print_function

__metaclass__ = type

import sys

from lpbuildd.target.operation import configure_logging
from lpbuildd.target.start import Start


def main():
    configure_logging()
    return Start().run()


if __name__ == "__main__":
    sys.exit(main())
